<filter jfrog.pipelines.**>
  @type parser
  key_name message
  <parse>
    @type json
  </parse>
</filter>
<filter jfrog.**>
  @type record_transformer
  <record>
    hostname "#{Socket.gethostname}"
    log_source ${tag}
  </record>
</filter>

########################
# ELASTIC OUTPUT
########################
<match jfrog.**>
  @type elasticsearch
  @id elasticsearch
  host my_host
  port 9200
  user my_user
  password my_password
  index_name unified-pipelines
  include_tag_key true
  type_name fluentd
  logstash_format false
</match>
#END ELASTIC OUTPUT